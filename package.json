{
  "name": "vegapunk-agentic",
  "version": "1.0.0",
  "description": "Autonomous multi-agent LLM system inspired by One Piece's Dr. Vegapunk satellites",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:unit": "jest --testPathPattern=tests/unit --coverage",
    "test:integration": "jest --testPathPattern=tests/integration --runInBand",
    "test:e2e": "jest --testPathPattern=tests/e2e --runInBand --detectOpenHandles",
    "test:performance": "jest --testPathPattern=tests/performance --runInBand --detectOpenHandles",
    "test:all": "ts-node scripts/test-runner.ts",
    "test:quick": "ts-node scripts/test-runner.ts --quick",
    "test:health": "ts-node scripts/test-runner.ts --health",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist coverage",
    "docker:build": "docker build -t vegapunk-agentic .",
    "docker:run": "docker run -it --rm vegapunk-agentic",
    "docker:dev": "docker-compose up -d",
    "precommit": "npm run lint && npm run format:check && npm run type-check && npm run test:unit",
    "validate": "npm run test:health && npm run test:quick",
    "ci": "npm run lint && npm run type-check && npm run test:unit && npm run test:integration"
  },
  "keywords": [
    "autonomous",
    "agents",
    "llm",
    "ai",
    "typescript",
    "agentic"
  ],
  "author": "Vegapunk Team",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.11.30",
    "@types/redis": "^4.0.11",
    "@types/uuid": "^9.0.8",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.3"
  },
  "dependencies": {
    "@a2a-js/sdk": "^0.2.5",
    "@genkit-ai/googleai": "^1.8.0",
    "@huggingface/inference": "^2.8.1",
    "@mistralai/mistralai": "^0.4.0",
    "@modelcontextprotocol/sdk": "^1.16.0",
    "@prisma/client": "^5.11.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "axios": "^1.6.8",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "eventemitter3": "^5.0.1",
    "express": "^5.1.0",
    "lodash": "^4.17.21",
    "neo4j-driver": "^5.18.0",
    "ollama": "^0.5.0",
    "openai": "^4.29.2",
    "prisma": "^5.11.0",
    "redis": "^4.6.13",
    "socket.io": "^4.7.4",
    "socket.io-client": "^4.7.4",
    "tsconfig-paths": "^4.2.0",
    "uuid": "^9.0.1",
    "winston": "^3.12.0",
    "ws": "^8.16.0",
    "yaml": "^2.4.1",
    "zod": "^3.22.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts",
      "!src/**/*.spec.ts",
      "!src/**/*.test.ts"
    ],
    "testMatch": [
      "**/__tests__/**/*.ts",
      "**/?(*.)+(spec|test).ts"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    }
  }
}
